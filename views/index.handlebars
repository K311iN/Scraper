<div>
    <h1 class="card 2-depth-3 center-align">Read the latest from 'The Verge'</h1>
    <div class="row">
        <div class="col s3"></div>
        <h5 class="card 2-depth-3 center-align col s6">Click 'Scape' to get the latest articles</h5>
    </div>
</div>
<div class="row">
    <div class="col s2"></div>
    <div class="col s8 left-align">
        {{#each article}}
        <div>
            <h4>{{this.title}}</h4>
            <a href="/readArticle/{{this_id}}" class="waves-effect waves-light btn"id="btn5SignUp">Read Articles</a>
        </div>
        {{/each}}
    </div>
</div>